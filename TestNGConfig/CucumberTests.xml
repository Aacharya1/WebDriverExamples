<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="WebDriverExamples" parallel="classes" thread-count="2">
    <listeners>
        <listener class-name="Test.Utils.ScreenShotListener"/>
    </listeners>

    <test name="WebDriverExamplesTest-Cucumber">
        <classes>
            <class name="cucumber.Steps.TheInternet.RunTheInternetCukesTest"/>
            <class name="cucumber.Steps.Google.RunGoogleCukesTest"/>
        </classes>
    </test>

</suite>